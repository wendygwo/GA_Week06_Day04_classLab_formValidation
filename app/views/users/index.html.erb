<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h1>All Users</h1>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>E-mail</th>
		</tr>
	</thead>
		<% @users.each do |user| %>
			<tr>
				<td><%=user.name%></td>
				<td><%=user.email%></td>
			</tr>

		<% end %>
</table>

<%=link_to 'Create a new user', new_user_path %>